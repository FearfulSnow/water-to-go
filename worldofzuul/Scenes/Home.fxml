<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="worldofzuul.Controllers.HomeController">
   <children>
      <Button layoutX="28.0" layoutY="308.0" mnemonicParsing="false" onAction="#goToFnRoom" text="FN Room" />
      <Button layoutX="61.0" layoutY="351.0" mnemonicParsing="false" onAction="#goToExploreRoom" text="Explore Room" />
      <Button layoutX="113.0" layoutY="308.0" mnemonicParsing="false" onAction="#goToWaterSource" text="Water Source" />
      <ImageView fx:id="homeImage" fitHeight="240.0" fitWidth="1026.0" layoutY="50.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Assets/HomeBackground.jpg" />
         </image>
      </ImageView>
      <fx:include fx:id="inventory" source="Inventory.fxml" />
   </children>
</AnchorPane>
