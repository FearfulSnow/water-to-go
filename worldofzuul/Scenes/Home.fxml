<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="worldofzuul.Controllers.HomeController">
    <children>
        <ImageView fitWidth="1600" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../Assets/HomeBackground.png" />
            </image>
        </ImageView>
        <fx:include fx:id="statusRow" source="Components/StatusRow.fxml" />
        <fx:include fx:id="navigation" source="Components/Navigation.fxml" />
        <fx:include fx:id="popup" source="Components/Popup.fxml" AnchorPane.rightAnchor="400" AnchorPane.leftAnchor="400" AnchorPane.topAnchor="300" />
    </children>
</AnchorPane>
