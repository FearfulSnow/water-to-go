<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.14-internal"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="worldofzuul.Controllers.HomeController">
    <children>
        <ImageView fx:id="homeImage" pickOnBounds="true"
                   preserveRatio="true" fitWidth="1600">
            <image>
                <Image url="@../Assets/HomeBackground.png"/>
            </image>
        </ImageView>
        <fx:include fx:id="statusRow" source="Components/StatusRow.fxml"/>
        <fx:include fx:id="navigation" source="Components/Navigation.fxml"/>
    </children>
</AnchorPane>
